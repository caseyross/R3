script.
	import Comment from './Comment.pug'
	import MoreComments from './MoreComments.coffee'
	export moreComments = {}

{#if moreComments instanceof MoreComments}
	{#await moreComments.comments}
		button(on:mousedown={moreComments.getComments()}) more
	{:then comments}
		{#each comments as comment}
			Comment(comment={comment})