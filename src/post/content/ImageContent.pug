script.
	import Image from '../../media/Image.pug'
	export content = {}

{#if content.type === 'image'}
	section
		{#each content.images as i}
			figure
				Image(image={i} maxW={640})
				{#if i.caption}
					figcaption {i.caption.t}
						{#if i.caption.u}
							a(href={i.caption.u}) {i.caption.u}